#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
void seguro(int a, int b)
{
	if(a==1)
	{
		printf("0\n");	
	}
	else
	{
		if(b==0)
		{
			printf("0\n");
		}
		else
		{
			printf("1\n");
		}
	}
}

int main() {
	int a, b;
	scanf("%d\n %d", &a, &b);
	seguro(a, b);
	return 0;
}
