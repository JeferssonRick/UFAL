#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	double r1, r2, area1, area2;
	scanf("%lf %lf",&r1, &r2);
	area1=(r1*r1)*3.14;
	area2=(r2*r2)*3.14;
	if(area1==area2)
	{
		printf("Iguais\n");
	}
	else if(area1>area2)
	{
		printf("Primeiro circulo\n");
	}
	else
	{
		printf("Segundo circulo\n");
	}
	return 0;
}
